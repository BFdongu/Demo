<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.MberMapper">

    <select id="searchPassword" parameterType="String" resultType="map">
        SELECT id, password
        FROM MBER_ACCOUNT_INFO
        WHERE id = #{id}
    </select>

    <select id="getAllMberAccount" resultType="map">
        SELECT id, password
        FROM MBER_ACCOUNT_INFO
    </select>

    <insert id="registerAccount">
        INSERT INTO MBER_ACCOUNT_INFO(id, password)
        VALUES(#{id}, #{password})
    </insert>

</mapper>
